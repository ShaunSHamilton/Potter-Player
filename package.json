{
    "name": "potter-player",
    "version": "1.0.1",
    "description": "A React App to play Potter's Wheel sermons",
    "main": "server/index.js",
    "scripts": {
        "babel-node": "babel-node ./server/index.js",
        "start:dev": "concurrently \"npm run server\" \"cd client && SET \"NODE_ENV=development\"  && npm start\"",
        "start:prod": "concurrently \"npm run server\" \"cd client && SET \"NODE_ENV=production\" && npm run build\"",
        "server": "nodemon --exec \"npm run babel-node\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sky020/potter-player.git"
    },
    "keywords": [
        "MERN Stack",
        "MongoDB",
        "Express",
        "React",
        "Node.js",
        "stack",
        "mern",
        "stack",
        "fullstack"
    ],
    "author": "Shaun Hamilton",
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "^7.1.2",
        "@babel/core": "^7.1.2",
        "@babel/node": "^7.0.0",
        "@babel/preset-env": "^7.1.0",
        "@babel/preset-react": "^7.0.0",
        "nodemon": "^1.18.4",
        "webpack": "^4.23.1",
        "webpack-dev-middleware": "^3.4.0",
        "webpack-hot-middleware": "^2.24.3"
    },
    "dependencies": {
        "@babel/register": "^7.0.0",
        "body-parser": "^1.18.3",
        "concurrently": "^4.0.1",
        "express": "^4.16.4",
        "mongoose": "^5.3.6"
    }
}